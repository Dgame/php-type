{
    "name": "dgame/php-type",
    "description": "php type",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-type",
    "authors": [
        {
            "name": "Randy Schuett ",
            "email": "rswhite4@gmail.com",
            "homepage": "https://github.com/php-type"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Dgame\\Type\\": "src/",
            "Dgame\\Test\\Type\\": "test/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpstan/phpstan": "^0.11.12",
        "phan/phan": "^2.2"
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse src --level=7",
            "phan"
        ]
    }
}
