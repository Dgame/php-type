{
    "name": "dgame/php-type",
    "description": "php type",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-type",
    "authors": [
        {
            "name": "Randy Schuett ",
            "email": "rswhite4@gmail.com",
            "homepage": "https://github.com/php-type"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Dgame\\Type\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "phpstan/phpstan": "^0.11.2"
    },
    "scripts": {
        "test": [
            "phpunit"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
